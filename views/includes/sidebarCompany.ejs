<script>
function checklist(){
  $('#flatbed_checklists_item').hide();
	$('#orientation_Checklists_item').hide();
	$('#dry_van_checklists_item').hide();
  $('.all_checklist').hide();
	$.ajax({
		url: "/getcompanychecklists",
		type: "GET",
		data: {},
		success: function (data) {
			$('.flatbed_checklists_item').hide();
			$('.orientation_Checklists_item').hide();
			$('.dry_van_checklists_item').hide();
			if (data.status) {
				if(data.data.flatbed_checklists==1){
					$('.flatbed_checklists_item').show();
          $('.all_checklist').show();
        }
				if(data.data.orientation_Checklists==1){	
					$('.orientation_Checklists_item').show();
          $('.all_checklist').show();
        }
				if(data.data.dry_van_checklists==1){
					$('.dry_van_checklists_item').show();
          $('.all_checklist').show();
        }
			}
      
		}
	});
}
function quiz(){
  $('.weightdimensions').hide();
  $('.safetylawsquiz').hide();
	$.ajax({
		url: "/getcompanyquiz",
		type: "GET",
		data: {},
		success: function (data) {
      $('.weightdimensions').hide();
      $('.safetylawsquiz').hide();
			if (data.status) {
        if(data.data.weightdimensions==1){
          $('.weightdimensions').show();
        }
        if(data.data.safetylawsquiz==1){
          $('.safetylawsquiz').show();
        }
			}
      
		}
	});
}
function cheklogo(){
  $.ajax({
		url: "/getcompanylogo",
		type: "GET",
		data: {company_id:''},
		success: function (data) {
			$(".light-logo").attr("src",data.data);
		}
	});
}
function disclaimer(){
	$('.sidebar-nav').show();
}
</script>